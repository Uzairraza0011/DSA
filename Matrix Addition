def matrix_addition(A, B):
    return [[A[0][0] + B[0][0], A[0][1] + B[0][1]],
            [A[1][0] + B[1][0], A[1][1] + B[1][1]]]

# Example usage
A = [[1, 2],
     [3, 4]]

B = [[5, 6],
     [7, 8]]

result = matrix_addition(A, B)
print("Matrix Addition Result:")
print(result)  # Output: [[6, 8], [10, 12]]
